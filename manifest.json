{
    "manifest_version": 2,
    "name": "Paystack Informant",
    "description": "Informs you  about your latest revenue and Payout",
    "version": "1.0",
    "permissions": ["https://70a22d02.ngrok.io/", "storage", "tabs"],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["jquery.js", "content.js"],
        "css": ["content.css"]
    }],
    "browser_action": {
        "default_title": "Paystack Informant",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
}